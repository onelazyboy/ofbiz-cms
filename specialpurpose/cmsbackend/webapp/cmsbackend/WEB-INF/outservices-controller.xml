<?xml version="1.0" encoding="UTF-8"?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/site-conf.xsd">
	<description>Common ControlServlet Configuration File</description>
    <errorpage>/error/error.jsp</errorpage>

	<!-- 外部数据接口 -->
	<request-map uri="findTest">
        <event type="java" path="org.ofbiz.webServers.TestServer" invoke="testResponse"/>
        <response name="success" type="view" value="none"/>
        <response name="error" type="view" value="error"/>
    </request-map>
</site-conf>